library(tidyverse)
library(janitor)
library(here)
library(naniar)
colleges <- readr::read_csv("data/ca_college_data.csv") %>% janitor::clean_names()
glimpse(colleges)
names(colleges)
naniar::miss_var_summary(colleges)
colleges %>%
count(city) %>%
arrange(desc(n))
colleges %>%
count(city) %>%
top_n(10, n) %>%
ggplot(aes(x=reorder(city,n), y=n))+
geom_col()+
coord_flip()
colleges %>%
group_by(city) %>%
summarize(mean_cost_yr=mean(costt4_a, na.rm=TRUE),
total=n()) %>%
arrange(desc(mean_cost_yr))
colleges %>%
filter(instnm== "University of California-Davis" | city=="Claremont" & costt4_a!="NA") %>%
ggplot(aes(x=instnm, y=costt4_a)) +
geom_col()+
coord_flip()
colleges %>%
ggplot(aes(x=adm_rate, y=c150_4_pooled))+
geom_point(na.rm=T)+
geom_smooth(method=lm, se=F, na.rm=T)
colleges %>%
ggplot(aes(x=costt4_a, y=c150_4_pooled))+
geom_point(na.rm=T)+
geom_smooth(method=lm, se=F, na.rm=T)
univ_calif <- colleges %>%
filter_all(any_vars(str_detect(., pattern = "University of California")))
univ_calif
univ_calif_final <- univ_calif %>%
filter(city!="San Francisco")
univ_calif_final
univ_calif_final <- univ_calif_final %>%
separate(instnm, c("univ", "campus"), sep="-")
univ_calif_final
univ_calif_final %>%
select(campus, adm_rate) %>%
arrange(adm_rate)
univ_calif_final %>%
ggplot(aes(x=reorder(campus, adm_rate), y=adm_rate))+
geom_col()+
coord_flip()
univ_calif_final %>%
select(campus, pcip26) %>%
arrange(pcip26)
univ_calif_final %>%
ggplot(aes(x=reorder(campus, pcip26), y=pcip26))+
geom_col()
