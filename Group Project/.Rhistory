knitr::opts_chunk$set(echo = TRUE)
library("tidyverse")
library("janitor")
library("lubridate")
happy <- read_csv("dataa/2021.csv") %>% clean_names()
happy
anyNA(happy)
table(happy$regional_indicator)
happy %>%
group_by(regional_indicator) %>%
summarize(mean_happiness=mean(ladder_score, na.rm=T)) %>%
ggplot(aes(x=regional_indicator, fill=mean_happiness))+
geom_bar(position="dodge")+
coord_flip()
happy %>%
group_by(regional_indicator) %>%
summarize(mean_gdp=mean(logged_gdp_per_capita, na.rm=T)) %>%
ggplot(aes(x=regional_indicator, y=mean_gdp, fill= mean_gdp))+
geom_col()+
coord_flip()
happy %>%
group_by(regional_indicator) %>%
summarize(mean_life_expectancy=mean(healthy_life_expectancy, na.rm=T)) %>%
ggplot(aes(x=regional_indicator, y=mean_life_expectancy, fill= regional_indicator))+
geom_col()+
coord_flip()
happy %>%
group_by(regional_indicator) %>%
summarize(mean_support=mean(social_support, na.rm=T)) %>%
ggplot(aes(x=regional_indicator, y=mean_support, fill=regional_indicator))+
geom_col()+
coord_flip()
happy %>%
select(country_name, ladder_score) %>%
arrange(desc(ladder_score))
happy %>%
select(country_name, ladder_score) %>%
arrange(ladder_score)
happy %>%
select(country_name, healthy_life_expectancy) %>%
arrange(desc(healthy_life_expectancy))
happy %>%
ggplot(aes(x=regional_indicator, y=freedom_to_make_life_choices))+
geom_boxplot()+
coord_flip()+
scale_y_log10()+
labs(title="Observations by Region",
x="Region",
y="Freedom")
